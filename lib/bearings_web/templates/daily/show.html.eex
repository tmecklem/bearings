<div class="row px-3">
  <%= if @previous_daily do %>
    <div class="container daily px-0 mt-0 mt-lg-4 px-3 col-lg-6 d-none d-lg-block">
      <%= render "daily.html", Map.put(assigns, :daily, @previous_daily) %>
    </div>
  <% end %>
  <div class="container daily px-0 mt-0 mt-md-4 px-3 col-lg-6">
    <%= render "daily.html", assigns %>
  </div>
</div>
<p></p>
